CREATE SCHEMA TestSchema;
GO

CREATE TABLE TestSchema.BlogEntry (
  Id       INT IDENTITY(1,1) NOT NULL PRIMARY KEY,
  Title     NVARCHAR(50),
  Content NVARCHAR(1200),
  CreatedDate int
);
GO


